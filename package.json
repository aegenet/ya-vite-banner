{
  "name": "@aegenet/ya-vite-banner",
  "description": "Yet Another Vite Banner",
  "version": "999.0.0",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    "node": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "default": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "typings": "./dist/bundle.d.ts",
  "bin": {
    "ya-vite-banner": "build/cli.js"
  },
  "files": [
    "build"
  ],
  "author": "Alexandre Genet",
  "repository": "https://github.com/aegenet/ya-vite-banner",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18"
  },
  "nyc": {
    "check-coverage": true,
    "exclude": "**/*.spec.ts",
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  },
  "devDependencies": {
    "@aegenet/ya-node-externals": "^0.1.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.7.17",
    "codecov": "^3.8.3",
    "json": "^11.0.0",
    "mocha": "^10.1.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-dts": "^5.0.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.3"
  },
  "dependencies": {
  },
  "pre-commit": [
    "lint"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "clean": "node ./node_modules/rimraf/bin ./build ./coverage ./.nyc_output ./node_modules",
    "test": "yarn tsc && nyc ts-mocha src/**/*.spec.ts --timeout 10000",
    "cover:report": "node ./node_modules/nyc/bin/nyc -t ./coverage report --reporter html --reporter json --reporter text-summary",
    "codecov": "./node_modules/.bin/codecov",
    "build:version": "node ./.build/build-flow.js build",
    "publish:version": "node ./.build/build-flow.js publish",
    "build": "yarn tsc && yarn run build:vite && yarn run build:dts",
    "tsc": "node ./node_modules/rimraf/bin build && node ./node_modules/typescript/bin/tsc",
    "build:vite": "node ./node_modules/rimraf/bin dist && node ./node_modules/vite/bin/vite build",
    "build:dts": "node ./node_modules/rollup/dist/bin/rollup -c rollup.config.dts.js"
  }
}
